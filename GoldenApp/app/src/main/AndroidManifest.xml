<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.goldenapp">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".GoldenAppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NovoProdutoActivity" />
        <activity android:name=".ProdutoActivity" />
        <activity
            android:name=".ConfiguracaoActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name=".LocalizacaoActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name=".MensagemActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name=".ForumActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name=".SettingsActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity
            android:name=".TelaInicialActivity"
            android:theme="@style/AppTheme.Toolbar" />
        <activity android:name=".DebugActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>